<form action='/auth/register' method='POST' class="logIn">
  <fieldset>
    <h2>Registrarse</h2>
    <label for="firstName">Nombre del usuario</label>
    <input type="text" id="firstName" name="firstName">
    <hr>
    <label for="lastName">Apellido del usuario</label>
    <input type="text" id="lastName" name="lastName">
    <hr>
    <label for="age">Edad del usuario</label>
    <input type="number" id="age" name="age">
    <hr>
    <label for="email">Email del usuario</label>
    <input type="email" id="email" name="email">
    <hr>
    <label for="password">Contrase√±a</label>
    <input type="password" id="password" name="password">
    <hr>
    <input type="submit" id="buttonSumbit">
  </fieldset>
</form>
